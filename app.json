{
  "expo": {
    "name": "Mindyatra",
    "slug": "mindyatra",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_new.png",
    "scheme": "mindyatraapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs microphone access for audio features"
      }
    },
"android": {
  "adaptiveIcon": {
    "foregroundImage": "./assets/images/logo_new.png",
    "backgroundColor": "#6366f1"
  },
  "edgeToEdgeEnabled": true,
  "predictiveBackGestureEnabled": false,
  "package": "com.gurups004.mindyatra",
  "versionCode": 1, 
  "versionName": "1.0.0",
  "permissions": [
    "RECORD_AUDIO",
    "MODIFY_AUDIO_SETTINGS"
  ]
},
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_new.png"
    },
    "splash": {
      "image": "./assets/images/logo_new.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "imageWidth": 200,
      "imageHeight": 200
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_new.png",
          "imageWidth": 200,
          "imageHeight": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#6366f1"
          }
        }
      ],
      [
       "expo-audio",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "5e72ce02-44fa-4e68-be2f-88b5e8ce4083"
      }
    },
    "owner": "gurups004"
  }
}